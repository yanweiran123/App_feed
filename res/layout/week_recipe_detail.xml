<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:paddingTop="10dip"
    >
    <RelativeLayout
      android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:paddingLeft="20dip"
        >
        <RelativeLayout
            android:id="@+id/foreHead1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            >
            <ImageView
                    android:id="@+id/imgCate1"
                    android:layout_height="30dip"
                    android:layout_width="30dip"
                    android:src="@drawable/canimg1"
                />
            <TextView
                android:id="@+id/category"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="早"
                android:textSize="16sp"
                android:textColor="@color/red"
                android:layout_below="@+id/imgCate1"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="5dip"
                />
        </RelativeLayout>
        <TextView
            android:id="@+id/content1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="包子 油条 豆浆"
            android:textSize="16sp"
            android:textColor="@color/black"
            android:layout_toRightOf="@+id/foreHead1"
            android:layout_centerVertical="true"
            android:layout_marginLeft="30dip"
            />
     </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="@color/grey1"
        android:layout_marginTop="2dp"
        android:layout_below="@+id/foreHead1"/>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/white"
            android:paddingLeft="20dip"
            android:layout_marginTop="5dp"
            >
            <RelativeLayout
                android:id="@+id/foreHead2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                >
                <ImageView
                    android:id="@+id/imgCate2"
                    android:layout_height="30dip"
                    android:layout_width="30dip"
                    android:src="@drawable/canimg2"
                    />
                <TextView
                    android:id="@+id/category"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="加"
                    android:textSize="16sp"
                    android:textColor="@color/red"
                    android:layout_below="@+id/imgCate2"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="5dip"
                    />
            </RelativeLayout>
            <TextView
                android:id="@+id/content2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="包子 油条 豆浆"
                android:textSize="16sp"
                android:textColor="@color/black"
                android:layout_toRightOf="@+id/foreHead2"
                android:layout_centerVertical="true"
                android:layout_marginLeft="30dip"
                />

        </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:layout_marginTop="2dp"
        android:background="@color/grey1"
        android:layout_below="@+id/foreHead2"/>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/white"
            android:paddingLeft="20dip"
            android:layout_marginTop="5dp"
            >
            <RelativeLayout
                android:id="@+id/foreHead3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                >
                <ImageView
                    android:id="@+id/imgCate3"
                    android:layout_height="30dip"
                    android:layout_width="30dip"
                    android:src="@drawable/canimg3"
                    />
                <TextView
                    android:id="@+id/category3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="午"
                    android:textSize="16sp"
                    android:textColor="@color/red"
                    android:layout_below="@+id/imgCate3"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="5dip"
                    />

            </RelativeLayout>
            <TextView
                android:id="@+id/content3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="包子 油条 豆浆"
                android:textSize="16sp"
                android:textColor="@color/black"
                android:layout_toRightOf="@+id/foreHead3"
                android:layout_centerVertical="true"
                android:layout_marginLeft="30dip"
                />
        </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="@color/grey1"
        android:layout_marginTop="2dp"
        android:layout_below="@+id/foreHead3"/>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/white"
            android:paddingLeft="20dip"
            android:layout_marginTop="5dp"
            >
            <RelativeLayout
                android:id="@+id/foreHead4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                >
                <ImageView
                    android:id="@+id/imgCate4"
                    android:layout_height="30dip"
                    android:layout_width="30dip"
                    android:src="@drawable/canimg4"
                    />
                <TextView
                    android:id="@+id/category4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="加"
                    android:textSize="16sp"
                    android:textColor="@color/red"
                    android:layout_below="@+id/imgCate4"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="5dip"
                    />
            </RelativeLayout>
            <TextView
                android:id="@+id/content4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="包子 油条 豆浆"
                android:textSize="16sp"
                android:textColor="@color/black"
                android:layout_toRightOf="@+id/foreHead4"
                android:layout_centerVertical="true"
                android:layout_marginLeft="30dip"
                />

        </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="@color/grey1"
        android:layout_marginTop="2dp"
        android:layout_below="@+id/foreHead4"/>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:paddingLeft="20dip"
        android:layout_marginTop="5dp"
        >
        <RelativeLayout
            android:id="@+id/foreHead5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            >
            <ImageView
                android:id="@+id/imgCate5"
                android:layout_height="30dip"
                android:layout_width="30dip"
                android:src="@drawable/canimg5"
                />
            <TextView
                android:id="@+id/category5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="晚"
                android:textSize="16sp"
                android:textColor="@color/red"
                android:layout_below="@+id/imgCate5"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="5dip"
                />
        </RelativeLayout>
        <TextView
            android:id="@+id/content5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="包子 油条 豆浆"
            android:textSize="16sp"
            android:textColor="@color/black"
            android:layout_toRightOf="@+id/foreHead5"
            android:layout_centerVertical="true"
            android:layout_marginLeft="30dip"
            />
    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="@color/grey1"
        android:layout_marginTop="2dp"
        android:layout_below="@+id/foreHead5"/>
</LinearLayout>